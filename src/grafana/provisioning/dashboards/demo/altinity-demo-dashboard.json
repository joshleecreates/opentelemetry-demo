          "format": "time_series",
          "formattedQuery": "SELECT $timeSeries as t, count() FROM $table WHERE $timeFilter GROUP BY t ORDER BY t",
          "interval": "",
          "intervalFactor": 1,
          "query": "SELECT $timeSeries AS t, max(cpu_utilization) as CPU, scope FROM\n(SELECT \n    TimeUnix,\n    toFloat64(TimeUnix) AS timestamp,\n    Attributes['type'] AS scope,\n    Value AS cpu_time,\n    any(cpu_time) OVER(PARTITION BY scope ORDER BY TimeUnix ROWS BETWEEN 1 PRECEDING AND 1 PRECEDING) as prev_cpu_time,\n    cpu_time-prev_cpu_time AS delta_cpu_time,\n    (timestamp - any(timestamp) OVER (PARTITION BY scope ORDER BY timestamp ROWS BETWEEN 1 PRECEDING AND 1 PRECEDING)) AS interval_seconds,\n    if(delta_cpu_time > 0 and interval_seconds > 0, delta_cpu_time / interval_seconds*100, 0) AS cpu_utilization\nFROM otel.otel_metrics_sum \nWHERE TimeUnix >= ($from - 200) AND TimeUnix <= $to\nAND MetricName='process.runtime.cpython.cpu_time' \nAND ServiceName='$service'\nORDER BY TimeUnix ASC\n)\nWHERE TimeUnix >= ($from-100) AND TimeUnix <= $to\nGROUP BY t, scope\nORDER BY t, scope",
          "rawQuery": "/* grafana dashboard=Demo Dashboard, user=0 */\nSELECT (intDiv(toFloat64(TimeUnix) * 1000, (1 * 1000)) * (1 * 1000)) AS t, max(cpu_utilization) as CPU, scope FROM\n(SELECT \n    TimeUnix,\n    toFloat64(TimeUnix) AS timestamp,\n    Attributes['type'] AS scope,\n    Value AS cpu_time,\n    any(cpu_time) OVER(PARTITION BY scope ORDER BY TimeUnix ROWS BETWEEN 1 PRECEDING AND 1 PRECEDING) as prev_cpu_time,\n    cpu_time-prev_cpu_time AS delta_cpu_time,\n    (timestamp - any(timestamp) OVER (PARTITION BY scope ORDER BY timestamp ROWS BETWEEN 1 PRECEDING AND 1 PRECEDING)) AS interval_seconds,\n    if(delta_cpu_time > 0 and interval_seconds > 0, delta_cpu_time / interval_seconds*100, 0) AS cpu_utilization\nFROM otel.otel_metrics_sum \nWHERE TimeUnix >= (1719336228 - 200) AND TimeUnix <= 1719336528\nAND MetricName='process.runtime.cpython.cpu_time' \nAND ServiceName='recommendationservice'\nORDER BY TimeUnix ASC\n)\nWHERE TimeUnix >= (1719336228-100) AND TimeUnix <= 1719336528\nGROUP BY t, scope\nORDER BY t, scope",
          "refId": "A",
          "round": "0s",
          "skip_comments": true,
          "table": "otel_metrics_sum"
        }
      ],
